<h2><%= user.firstname %></h2>

<h3> in <%= user.location %></h3>

<div class="row">
  <div class="col-sm-6">
    <h3>Interests</h3>
    <% user.tags.forEach(function(tag){ %>
      <div class="well">
          <%= tag.name %>
      </div>
    <% }); %>
  </div>
  <div class="col-sm-6">
    <h3>Skills</h3>
    <% user.skills.forEach(function(skill){ %>
      <div class="well">
        <p>
          <%= skill.content %>
        </p>
      </div>
    <% }); %>
  </div>
</div>
